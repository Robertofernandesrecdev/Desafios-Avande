using System;

class Desafio {
    static void Main() {
        int x=60 , y= 90;
        int quilometros = Int32.Parse(Console.ReadLine());
        int minutos = quilometros / (y -x) * 60; // Digite aqui o calculo dos minutos
        Console.WriteLine(minutos + " minutos");
    }
}






